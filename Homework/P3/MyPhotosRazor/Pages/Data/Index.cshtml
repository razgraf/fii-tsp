@page
@model MyPhotosRazor.Pages.Data.IndexModel
@{
    ViewData["Title"] = "Index";
}

<h1>Files</h1>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Datas[0].DataId)</th>
            <th>@Html.DisplayNameFor(model => model.Datas[0].Label)</th>
            <th>@Html.DisplayNameFor(model => model.Datas[0].Type)</th>
            <th>@Html.DisplayNameFor(model => model.Datas[0].IsEditable)</th>
            <th>@Html.DisplayNameFor(model => model.Datas[0].CreatedAt)</th>
  
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Datas)
        {
        <tr style="font-size:small">
            <td>@Html.DisplayFor(modelItem => item.DataId)</td>
            <td>@Html.DisplayFor(modelItem => item.Label)</td>
            <td>@Html.DisplayFor(modelItem => item.Type)</td>
            <td>@Html.DisplayFor(modelItem => item.IsEditable)</td>
            <td>@Html.DisplayFor(modelItem => item.CreatedAt)</td>
    
        </tr>
        }
    </tbody>
</table>

