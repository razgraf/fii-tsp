@page "{id:int?}"
@model MyPhotosRazor.Pages.FileData.IndexModel
@{
    ViewData["Title"] = "Index";
}

<h1>Files</h1>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].FileDataId)</th>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].FileId)</th>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].DataId)</th>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].DataName)</th>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].Value)</th>
            <th>@Html.DisplayNameFor(model => model.FileDatas[0].CreatedAt)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.FileDatas)
        {
        <tr style="font-size:small">
            <td>@Html.DisplayFor(modelItem => item.FileDataId)</td>
            <td>@Html.DisplayFor(modelItem => item.FileId)</td>
            <td>@Html.DisplayFor(modelItem => item.DataId)</td>
            <td>@Html.DisplayFor(modelItem => item.DataName)</td>
            <td>@Html.DisplayFor(modelItem => item.Value)</td>
            <td>@Html.DisplayFor(modelItem => item.CreatedAt)</td>
        </tr>
        }
    </tbody>
</table>
